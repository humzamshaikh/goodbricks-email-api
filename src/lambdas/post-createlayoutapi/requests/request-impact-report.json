  "body": "{\"name\":\"impact-report\",\"description\":\"Annual impact report email template showing community impact metrics and achievements\",\"category\":\"newsletters\",\"jsxCode\":\"import {\\n  Body,\\n  Column,\\n  Container,\\n  Head,\\n  Heading,\\n  Hr,\\n  Html,\\n  Link,\\n  Preview,\\n  Row,\\n  Section,\\n  Tailwind,\\n  Text,\\n} from '@react-email/components';\\n\\ninterface ImpactReportEmailProps {\\n  year?: number;\\n  livesImpacted?: number;\\n  communitiesServed?: number;\\n  volunteersEngaged?: number;\\n  eventsHosted?: number;\\n  totalHours?: number;\\n  topProgramName?: string;\\n  topProgramImpact?: number;\\n  mostActiveMonth?: string;\\n  mostActiveMonthEvents?: number;\\n  impactPercentile?: number;\\n  serviceLocations?: string[];\\n  firstName?: string;\\n  lastName?: string;\\n}\\n\\nexport default function ImpactReportEmail({\\n  year,\\n  livesImpacted,\\n  communitiesServed,\\n  volunteersEngaged,\\n  eventsHosted,\\n  totalHours,\\n  topProgramName,\\n  topProgramImpact,\\n  mostActiveMonth,\\n  mostActiveMonthEvents,\\n  impactPercentile,\\n  serviceLocations = [],\\n  firstName,\\n  lastName,\\n}: ImpactReportEmailProps) {\\n  return (\\n    <Html>\\n      <Head />\\n      <Preview>See the impact we made together in {year}</Preview>\\n      <Tailwind>\\n        <Body className=\\\"bg-white font-sans\\\">\\n          <Container className=\\\"mx-auto w-full max-w-[600px] p-0\\\">\\n            <Section className=\\\"p-8 text-center\\\">\\n              <Text className=\\\"mx-0 mt-4 mb-8 p-0 text-center font-normal text-2xl\\\">\\n                <span className=\\\"font-bold tracking-tighter\\\">GoodBricks</span>\\n              </Text>\\n              <Text className=\\\"font-normal text-sm uppercase tracking-wider\\\">\\n                {year} Impact Report\\n              </Text>\\n              <Heading className=\\\"my-4 font-medium text-4xl leading-tight\\\">\\n                Dear {firstName} {lastName},\\n              </Heading>\\n              <Text className=\\\"mb-8 text-lg leading-8\\\">\\n                What an incredible year it&apos;s been! Together, we&apos;ve made a real difference in our community. \\n                Let&apos;s celebrate the impact we&apos;ve created together.\\n              </Text>\\n              <Link\\n                href=\\\"https://www.goodbricks.org\\\"\\n                className=\\\"inline-flex items-center rounded-full bg-gray-900 px-12 py-4 text-center font-bold text-sm text-white no-underline\\\"\\n              >\\n                View Full Impact Report\\n              </Link>\\n            </Section>\\n\\n            <Section className=\\\"my-6 rounded-2xl bg-[#fb7a00]/10 bg-[radial-gradient(circle_at_bottom_right,#fb7a00_0%,transparent_60%)] p-8 text-center\\\">\\n              <Heading className=\\\"m-0 font-medium text-3xl text-[#a63b00]\\\">\\n                Lives Impacted\\n              </Heading>\\n              <Text className=\\\"my-4 font-bold text-7xl text-gray-900 leading-none\\\">\\n                {livesImpacted}\\n              </Text>\\n              <Text className=\\\"mb-4 font-medium text-3xl text-gray-900\\\">\\n                people reached through our programs\\n              </Text>\\n              <Text className=\\\"text-gray-900 text-sm leading-5\\\">\\n                Every number represents a real person whose life has been touched by our community&apos;s generosity and dedication.\\n              </Text>\\n\\n              <Hr className=\\\"mt-6\\\" style={{ borderColor: '#fb7a00' }} />\\n              <Heading className=\\\"pt-5 font-medium text-gray-900 text-xs uppercase tracking-wider\\\">\\n                Our Collective Impact\\n              </Heading>\\n              <Row className=\\\"mt-5\\\">\\n                <Column className=\\\"w-1/3 text-center\\\">\\n                  <Text className=\\\"font-medium text-[#a63b00] text-sm\\\">\\n                    Communities Served\\n                  </Text>\\n                  <Text className=\\\"my-1 font-bold text-4xl text-gray-900\\\">\\n                    {communitiesServed}\\n                  </Text>\\n                  <Text className=\\\"text-2xl text-gray-900\\\">locations</Text>\\n                </Column>\\n                <Column className=\\\"w-1/3 text-center\\\">\\n                  <Text className=\\\"font-medium text-[#a63b00] text-sm\\\">\\n                    Volunteers Engaged\\n                  </Text>\\n                  <Text className=\\\"my-1 font-bold text-4xl text-gray-900\\\">\\n                    {volunteersEngaged}\\n                  </Text>\\n                  <Text className=\\\"text-2xl text-gray-900\\\">people</Text>\\n                </Column>\\n                <Column className=\\\"w-1/3 text-center\\\">\\n                  <Text className=\\\"font-medium text-[#a63b00] text-sm\\\">\\n                    Events Hosted\\n                  </Text>\\n                  <Text className=\\\"my-1 font-bold text-4xl text-gray-900\\\">\\n                    {eventsHosted}\\n                  </Text>\\n                  <Text className=\\\"text-2xl text-gray-900\\\">events</Text>\\n                </Column>\\n              </Row>\\n            </Section>\\n\\n            <Section className=\\\"my-6 rounded-2xl bg-[#4b5563]/10 bg-[radial-gradient(circle_at_bottom_right,#4b5563_0%,transparent_60%)] p-8 text-center\\\">\\n              <Heading className=\\\"m-0 font-medium text-3xl text-gray-800\\\">\\n                Our Most Impactful Program\\n              </Heading>\\n              <Text className=\\\"my-4 font-bold text-2xl text-gray-900 leading-none\\\">\\n                &quot;{topProgramName}&quot;\\n              </Text>\\n              <Text className=\\\"mb-4 font-medium text-5xl text-gray-900\\\">\\n                {topProgramImpact} lives changed\\n              </Text>\\n              <Text className=\\\"text-gray-900 text-sm leading-5\\\">\\n                This program 